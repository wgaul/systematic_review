@article{Archibald2017,
abstract = {Most landbirds migrate at night and typically make landfall in suitable$\backslash$nstopover habitat before dawn. However, when birds find themselves over$\backslash$nlarge water bodies at dawn, they must continue flying into the day and$\backslash$neither finish crossing the water body and land on the far shore or$\backslash$nbacktrack to the near (i.e. first encountered) shore to land. Their$\backslash$ncollective decisions will influence how migrants are distributed among$\backslash$nshoreline stopover habitats on either side of the water crossing. We$\backslash$nstudied birds during 4 spring migration seasons from 2010 to 2013 in the$\backslash$nGreat Lakes region, USA. We used 3 weather surveillance radars to$\backslash$nobserve migrating landbirds' behavior at dawn and subsequent terrestrial$\backslash$ndistributions during stopover. Mean flight heights over land and water$\backslash$nwere higher and mean flight directions were more oriented toward the$\backslash$nclosest shore at dawn when compared to peak migration earlier in the$\backslash$nnight. The wider the lake crossing, the higher that birds along the$\backslash$nlakeshore flew at dawn. Seasonal mean stopover densities of migrants on$\backslash$nland within 3 km of shorelines were 48{\%} higher on the near shores of$\backslash$nlakes (based on the seasonal mean direction of migration) than on the$\backslash$nfar shores. There was a moderate positive correlation (r = 0.584, P{\textless}$\backslash$n0.001, n = 358) between the seasonal mean density of birds aloft over$\backslash$nwater at dawn and the stopover density of birds in adjacent shorelines.$\backslash$nThus, birds over the water at dawn may tend to return to the near shore$\backslash$nof the Great Lakes for stopover rather than continuing across the water.$\backslash$nAs a result, shoreline habitats on the near shores of lakes harbor$\backslash$ngreater densities of migrants and are thus critical stopover sites for$\backslash$nmigrating landbirds in the Great Lakes region.},
annote = {Did this for the podcast in early 2018. Would be cool to use eBird to test bird densities on near vs. far shores.},
author = {Archibald, Kevin M and Buler, Jeffrey J and Smolinsky, Jaclyn A and Smith, Robert J},
doi = {10.1642/auk-16-123.1},
issn = {0004-8038},
journal = {The Auk},
keywords = {bird,citizen science,eBird,migration,podcast},
mendeley-tags = {bird,citizen science,eBird,migration,podcast},
month = {jan},
number = {1},
pages = {193--201},
publisher = {American Ornithologists Union},
title = {{Migrating birds reorient toward land at dawn over the Great Lakes, USA}},
volume = {134},
year = {2017}
}
