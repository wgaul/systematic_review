@article{Abellan2008,
abstract = {BACKGROUND: The use of Bayesian hierarchical spatial models has become widespread in disease mapping and ecologic studies of health-environment associations. In this type of study, the data are typically aggregated over an extensive time period, thus neglecting the time dimension. The output of purely spatial disease mapping studies is therefore the average spatial pattern of risk over the period analyzed, but the results do not inform about, for example, whether a high average risk was sustained over time or changed over time. OBJECTIVE: We investigated how including the time dimension in disease-mapping models strengthens the epidemiologic interpretation of the overall pattern of risk. METHODS: We discuss a class of Bayesian hierarchical models that simultaneously characterize and estimate the stable spatial and temporal patterns as well as departures from these stable components. We show how useful rules for classifying areas as stable can be constructed based on the posterior distribution of the space-time interactions. We carry out a simulation study to investigate the sensitivity and specificity of the decision rules we propose, and we illustrate our approach in a case study of congenital anomalies in England. RESULTS: Our results confirm that extending hierarchical disease-mapping models to models that simultaneously consider space and time leads to a number of benefits in terms of interpretation and potential for detection of localized excesses.},
annote = {Only skimmed it, but they partition study area with a grid with varying sized cells similar to Fink et al's STEM as a way to deal with spatially uneven occurrence records.},
author = {Abellan, Juan Jose and Richardson, Sylvia and Best, Nicky},
doi = {10.1289/ehp.10814},
issn = {00916765},
journal = {Environmental Health Perspectives},
keywords = {Bayesian hierarchical models,Congenital anomalies,Disease mapping,Mixture models,PhD,Space time interactions,Stable disease patterns,recording bias,spatial bias,spatial-temporal model},
mendeley-tags = {PhD,recording bias,spatial bias,spatial-temporal model},
month = {apr},
number = {8},
pages = {1111--1119},
publisher = {Environmental Health Perspectives},
title = {{Use of space time models to investigate the stability of patterns of disease}},
volume = {116},
year = {2008}
}
